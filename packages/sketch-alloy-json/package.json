{
  "name": "sketch-alloy-json",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "main": "dist/src/index.js",
  "scripts": {
    "build-src": "babel src -d dist/src --extensions .ts,.tsx",
    "build": "tsc -p . && npm run build-src",
    "clean": " rimraf ./dist",
    "watch-src": "babel src -d dist/src --extensions .ts,.tsx --watch",
    "watch-tsc": "tsc -p . --watch",
    "watch": "concurrently --kill-others \"npm run watch-tsc\" \"npm run watch-src\"",
    "test": "vitest run --exclude=test/e2e",
    "build-sample": "rimraf sample/output && tsp compile sample/main.tsp --emit sketch-alloy-json --output-dir sample/output",
    "format": "prettier . --write"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "peerDependencies": {
    "@typespec/compiler": "workspace:*",
    "@typespec/http": "workspace:*",
    "@typespec/openapi": "workspace:*",
    "@typespec/rest": "workspace:*",
    "@typespec/emitter-framework": "workspace:*",
    "@typespec/json-schema": "workspace:*",
    "@typespec/openapi3": "workspace:*"
  },
  "dependencies": {
    "@alloy-js/core": "^0.5.0",
    "@alloy-js/json": "^0.5.0"
  },
  "devDependencies": {
    "chalk": "^2.4.2",
    "execa": "^9.5.2",
    "globby": "^14.0.2",
    "fs-extra": "^11.2.0",
    "yargs": "^17.7.2",
    "@types/yargs": "^17.0.0",
    "inquirer": "^12.2.0",
    "@typespec/ts-http-runtime": "0.1.0",
    "uri-template": "^2.0.0",
    "@typespec/http-specs": "workspace:~",
    "@typespec/versioning": "workspace:~",
    "@azure-tools/typespec-client-generator-core": "0.49.0",
    "@azure-tools/typespec-azure-resource-manager": "0.49.0",
    "@azure-tools/cadl-ranch": "0.16.2",
    "@azure-tools/cadl-ranch-expect": "0.15.7",
    "@azure-tools/cadl-ranch-specs": "0.39.6",
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/preset-typescript": "^7.26.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@alloy-js/babel-preset": "^0.1.1",
    "concurrently": "^8.2.2",
    "ts-morph": "^23.0.0",
